r.vue'\nexport default {\n    components:{ingredContainer},\n    inject:['selectedIngred','clicked'],\n    data(){\n        return{\n            ingContainer:[]\n        }\n    },\n    mounted(){\n        for(let i=0;i<20;i++){\n            let object =new Object();\n            object.ingName=undefined;\n            object.img=undefined;\n            this.ingContainer.push(object)\n        }\n    },\n    computed:{\n        something:function(){\n            this.selectedIngred.forEach((d,i)=>{\n            if(!this.ingContainer[i].ingName){\n                console.log('child receives ',d.name)\n                this.ingContainer[i].ingName=this.selectedIngred[i].name\n                this.ingContainer[i].img=this.selectedIngred[i].img;\n            }else{\n                console.log('something goes wrong')\n            }\n        })\n        return this.ingContainer\n        }\n    },\n    watch:{\n        selectedIngred:{\n            handler:function(){\n                let selectedLength=this.selectedIngred.length;\n                let comparedLength=this.ingContainer.filter(d=>d.ingName).length;\n                console.log('selectlength',selectedLength);\n                console.log('compredLength is ', comparedLength)\n                console.log(this.ingContainer.filter(d=>d.ingName));\n                \n                if(selectedLength<comparedLength){\n\n                    this.ingContainer.forEach((d,i)=>{\n                        // if(d.ingName==this.clicked){\n                        //     let index=i;\n                        //     this.ingContainer[index].ingName=undefined;\n                        //     this.ingContainer[index].img=undefined;\n                        // }\n                        d.ingName=undefined;\n                        d.img=undefined;\n                        console.log('you just clicked',this.clicked)\n                    });\n\n                }\n            },\n            deep:true\n\n        }\n        // clicked:{\n        //     handler:function(){\n        //         console.log('you just clicked',this.clicked[0])\n        //     },\n        //     deep:true\n        // }\n    }\n}\n</script>\n\n<style scoped>\n.side{\n    width:25vw;\n    height:100vh;\n    background-color:#D9D1C720;\n    padding-top:15px;\n    text-align:center;\n    overflow-y:hidden;\n\n}\n.sidesection{\n\n    padding-top:15px;\n    overflow-y:scroll;\n    height:100%;\n    display:grid;\n    grid-template-columns: repeat(2,1fr);\n    grid-template-rows:auto;\n    gap:30px 10px;\n    z-index:5;\n}\n.counter{\n    width:100%;\n    height:35px;\n    display:flex;\n    gap:0px 10px;\n    align-items: center;\n    color:#D9D1C7;\n    margin-bottom:20px;\n}\n</style>\n\n"],"sourceRoot":""}]}