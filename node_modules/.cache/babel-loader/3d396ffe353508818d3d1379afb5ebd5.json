/     handler:function(){\n        //         console.log('you just clicked',this.clicked[0])\n        //     },\n        //     deep:true\n        // }\n    }\n}\n</script>\n\n<style scoped>\n.side{\n    width:25vw;\n    height:100vh;\n    background-color:#D9D1C720;\n    padding-top:15px;\n    text-align:center;\n    overflow-y:hidden;\n\n}\n.sidesection{\n\n    padding-top:15px;\n    overflow-y:scroll;\n    height:100%;\n    display:grid;\n    grid-template-columns: repeat(2,1fr);\n    grid-template-rows:auto;\n    gap:30px 10px;\n    z-index:5;\n}\n.counter{\n    width:100%;\n    height:35px;\n    display:flex;\n    gap:0px 10px;\n    align-items: center;\n    color:#D9D1C7;\n    margin-bottom:20px;\n}\n</style>\n\n"],"sourceRoot":""}]}