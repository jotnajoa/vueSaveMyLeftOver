ollection=chunkMaxLength(a,5,4)\n            let pageNumber=Math.floor(this.selectedIngred.length/5)+1;\n            this.initial=pageNumber\n            let filtered =this.ingredCollection.filter((d)=>!d.length==0)\n            this.ingredCollection=filtered;\n      },\n    deep:true\n    }\n  }\n}\n\n</script>\n<style scoped>\n\n.ingimg{\n    width:60px;\n    height:65px;\n  }\n\n.carousel{\n\n  text-align:center;\n  position:relative;\n  width:100%;\n  /* border:solid 1px green; */\n  margin:auto;\n\n}\n.containers{\n  display:grid;\n  grid-auto-flow: column;\n  border-radius:70px;\n  border:solid 1px grey;\n  align-items: start;\n  align-content: center;\n  height:95%;\n  width:99.5%;\n\n\n}\n.elements{\n  height:100%;\n  width:100%;\n  position:relative;\n  text-align:center;\n  cursor: pointer;\n}\n.names{\n  color:#D9D1C7;\n  width:100%;\n  height:20px;\n  /* overflow-y:hidden; */\n  overflow: hidden; \n  position:absolute;\n  top:90%;\n  left:50%;\n  transform:translate(-50%);\n  white-space: nowrap;\n  text-overflow: ellipsis;\n}\n\n\n  .el-carousel__item:nth-child(2n),.el-carousel__item:nth-child(2n+1){\n    background-color: #39414B;\n  }\n\n\n.bottomcontainer{\n  width:100%;\n  position:absolute;\n  bottom:5%;\n}\n\n\n\n</style>"],"sourceRoot":""}]}