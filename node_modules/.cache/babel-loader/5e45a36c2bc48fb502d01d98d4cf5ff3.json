ick=\"receiveIngredients(ingname,imgfile)\"\n        @mouseover=\"hovered=true\"\n        @mouseleave=\"hovered=false\"\n        :src=\"imgfile\" \n        class='ingimg'>\n    </div>\n    <div class='modal' v-if='hovered'>\n        {{ingname}}\n    </div>\n    </div>\n</template>\n\n<script>\n\nexport default {\n\n    props:['ingname','index'],\n    inject:['receiveIngredients','allData'],\n    data(){\n        return{\n            blocks:['blblock.png','brblock.png','rdblock.png','ylblock.png'],\n            hovered:false\n        }\n    },\n    updated(){\n        // console.log('hi')\n        // console.log(this.blocks)\n    },\n    computed:{\n        imgfile(){\n            return (this.index%4==0 ? this.blocks[0]:\n            this.index%4==1 ? this.blocks[1]:\n            this.index%4==2 ? this.blocks[2]:\n            this.index%4==3 ? this.blocks[3]:null\n            )\n        }\n    }\n\n    \n}\n</script>\n<style scoped>\n.modal{\n    position:absolute;\n    top:100%;\n    left:0px;\n    background-color:#1c1c1c84;\n    border:#BFA863 solid 1px;\n}\n.blockbox{\n    color:#D9D1C7;\n    width:120px;\n    height:100px;\n}\nimg{\n    display:block;\n    width:70px;\n    height:70px;\n    cursor: pointer;\n}\n.names{\n    /* overflow:hidden; */\n    border-left:solid 1px #D9D1C7;\n    padding-left:5px;\n    height:50px;\n    width:80%;\n    overflow: hidden;   \n    white-space: nowrap;\n    text-overflow: ellipsis;\n}\n</style>"],"sourceRoot":""}]}