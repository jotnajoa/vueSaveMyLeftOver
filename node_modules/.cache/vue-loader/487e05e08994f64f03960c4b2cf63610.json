,CAAC,CAAC;IACnC,CAAC,CAAC,CAAC,CAAC,CAAC","file":"/Users/soonkpaik/Desktop/vueSaveMyLeftOver/src/components/centerpiece/gamecanvas/centercanvas.vue","sourceRoot":"","sourcesContent":["<template>\n    <div style='border:solid 1px yellow' v-bind:style=\"{ width: `${width}px`, height: `${height}px`}\">\n        <canvas id='interface'></canvas>\n    </div>\n</template>\n<script>\nimport IngCircle from './ingcircle.js'\nexport default {\nprops:['width','height'],\ninject:['allData','ingCollection','selectedIngred'],\n// 전체 레시피 데이터를 받아야된다\ndata() {\n    return {\n        circlearray:[],\n        canvas:null,\n        context:undefined,\n        orbitradius:this.width*1/3,\n        speed:2\n    }\n},\nmounted(){\n    let canvas = document.querySelector('#interface');\n    let context = canvas.getContext('2d');\n    this.canvas=canvas;\n    this.context=context;\n    this.canvas.width=this.width;\n    this.canvas.height=this.height;\n\n    this.allData.forEach((d,i)=>{\n        console.log(d,i);\n        this.circlearray.push(\n            new IngCircle(this.context,this.width/2,this.height/2,i,i,this.orbitradius,this.speed,this.speed,d)\n            )\n        })\n    setTimeout(()=>{\n    this.circlearray.forEach((d)=>{\n        d.draw();\n    })\n    },1000)\n}\n}\n</script>\n\n"]}]}